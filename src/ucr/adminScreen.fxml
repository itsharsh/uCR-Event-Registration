<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucr.adminScreenController">
   <children>
      <GridPane alignment="CENTER" layoutX="5.0" layoutY="3.0" prefHeight="400.0" prefWidth="400.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <VBox alignment="CENTER" prefHeight="214.0" prefWidth="199.0" spacing="10.0" GridPane.rowIndex="1">
               <children>
                  <Button alignment="CENTER" mnemonicParsing="false" prefWidth="135.0" text="Create Event" />
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#addData" prefWidth="135.0" text="Add Data" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="0.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="1">
               <children>
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#adminLogout" prefWidth="110.0" text="Log Out" />
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#addUser" prefWidth="110.0" text="Add User" />
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#deleteUser" prefWidth="110.0" text="Delete User" />
               </children>
            </VBox>
            <VBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#viewData" prefWidth="135.0" text="View Data" />
                  <Button alignment="CENTER" layoutX="43.0" layoutY="74.0" mnemonicParsing="false" onAction="#exportData" prefWidth="135.0" text="Export Data" />
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#viewStatistics" prefWidth="135.0" text="View Statistics" />
               </children>
            </VBox>
         </children>
      </GridPane>
   </children>
</AnchorPane>
